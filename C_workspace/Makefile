.PHONY: clean All

All:
	@echo "----------Building project:[ Progetto_algoritmi_CPU - Debug ]----------"
	@cd "Progetto_algoritmi_CPU" && "$(MAKE)" -f  "Progetto_algoritmi_CPU.mk"
clean:
	@echo "----------Cleaning project:[ Progetto_algoritmi_CPU - Debug ]----------"
	@cd "Progetto_algoritmi_CPU" && "$(MAKE)" -f  "Progetto_algoritmi_CPU.mk" clean
