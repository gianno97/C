.PHONY: clean All

All:
	@echo "----------Building project:[ Matrix_project - Debug ]----------"
	@cd "Matrix_project" && "$(MAKE)" -f  "Matrix_project.mk"
clean:
	@echo "----------Cleaning project:[ Matrix_project - Debug ]----------"
	@cd "Matrix_project" && "$(MAKE)" -f  "Matrix_project.mk" clean
