.PHONY: clean All

All:
	@echo "----------Building project:[ Input_validation - Debug ]----------"
	@cd "Input_validation" && "$(MAKE)" -f  "Input_validation.mk"
clean:
	@echo "----------Cleaning project:[ Input_validation - Debug ]----------"
	@cd "Input_validation" && "$(MAKE)" -f  "Input_validation.mk" clean
