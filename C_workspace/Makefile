.PHONY: clean All

All:
	@echo "----------Building project:[ Cerca_albero_bin_ric - Debug ]----------"
	@cd "Cerca_albero_bin_ric" && "$(MAKE)" -f  "Cerca_albero_bin_ric.mk"
clean:
	@echo "----------Cleaning project:[ Cerca_albero_bin_ric - Debug ]----------"
	@cd "Cerca_albero_bin_ric" && "$(MAKE)" -f  "Cerca_albero_bin_ric.mk" clean
